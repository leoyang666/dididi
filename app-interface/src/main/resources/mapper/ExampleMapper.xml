<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.codingforhappy.dao.sql.ExampleDao">
    <insert id="insertPassengers" parameterType="java.util.Map">
        insert into passengers (id, phonenum, password, nickname)
        values (#{id}, #{phonenum}, #{password}, #{nickname})
    </insert>
</mapper>